{
  "name": "cloudinary/cloudinary_php",
  "version": "2.0.0-beta2",
  "description": "Cloudinary PHP SDK v2",
  "keywords": [
    "cloudinary",
    "sdk",
    "cloud",
    "image management",
    "cdn"
  ],
  "type": "library",
  "homepage": "https://github.com/cloudinary/cloudinary_php/tree/2.0.0-beta",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Cloudinary",
      "homepage": "https://github.com/cloudinary/cloudinary_php/graphs/contributors"
    }
  ],
  "support": {
    "email": "info@cloudinary.com",
    "issues": "https://github.com/cloudinary/cloudinary_php/issues"
  },
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": "^6",
    "shrikeh/teapot": "^1",
    "ext-json": "*",
    "monolog/monolog": "^1.24"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "phpdocumentor/phpdocumentor": "2.*",
    "jms/serializer": "1.7.*",
    "phpmd/phpmd": "*",
    "squizlabs/php_codesniffer": "3.*",
    "friendsofphp/php-cs-fixer": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-zip": "*",
    "consolidation/robo": "~1"
  },
  "autoload": {
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  }
}
